{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": [
          "./README.md",
          "lib/",
          "index.js"
        ]
    },
    "plugins": [
        "plugins/markdown", 
         "plugins/underscore"
    ],
    "opts": {
        "template": "node_modules/docdash/",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "docdash": { 
        "static": true,
        "sort": true, 
        "disqus": "",
      "openGraph": { 
        "title": "SimplyEco Docs",
        "type": "website",
        "image": "https://i.imgur.com/DWeejI6.png",
        "site_name": "SimplyEco", 
        "url": "https://simplyeco.js.org"

      }, 
      "meta": { 
        "title": "SimplyEco", 
        "description": "Simply Eco is a powerful module that allows you to create economy system in your bot with ease.:)",
        "keyword": "simplyeco, mongo-eco.js, economy-mongo, eco, economy, simply-eco"
        }, 
      "search": true,
      "typedefs": true,
      "removeQuotes": "none",
      "scripts": [], 
  "menu": {
  "Github repo": { 
  "href":"https://github.com/Xx-Mohit-xX/simply-eco",
  "target":"_blank", 
  "class":"menu-item", 
  "id":"repository" 
  },
  "Npm Info": {
  "href":"https://www.npmjs.com/package/simply-eco",
  "target":"_blank",
  "class":"menu-item",
  "id":"npm" 
  },
  "Support server": {
"href":"https://discord.gg/NAaxz5PgNn",
  "target":"_blank",
  "class":"menu-item",
  "id":"server"     
  }
} 
}
}